{
  "compilerOptions": {
    "target": "es6",
    "allowSyntheticDefaultImports": false,
    "baseUrl": ".",
    "paths": {
      "@root/*": ["./*"],
      "@modules/*": ["./src/redux/modules/*"],
      "@s/*": ["./src/services/*"],
      "@a/*": ["./src/assets/*"],
      "@common/*": ["./src/common/*"],
      "@constants/*": ["./src/constants/*"]
    }
  },
  "exclude": ["node_modules"]
}
